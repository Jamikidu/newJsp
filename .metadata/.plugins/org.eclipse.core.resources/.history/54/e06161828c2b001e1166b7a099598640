package basic_p;

public class TryCatch_369 {

	public static void main(String[] args) {
		
		for(int i=1;i<=20;i++) {
			int one = i % 10; // 1~10
			
			int num1 = one % 3;   
			System.out.println(num1);
			
			try {
				System.out.println(i);
			} catch (Exception e) {
				System.out.println("짝");
			}
		}
	}

}

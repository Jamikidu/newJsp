package basic_p;

public class TSNMain {

	public static void main(String[] args) {
		
		System.out.println("3,6,9 게임 시작!!");
		System.out.println("1부터 -> 20까지 시작");
		//for(초기값; 조건; 증감식){}
		for(int i= 1; i<20; i++) {
			int one = i%10;
			if(one%3==0 && one != 0) {
				System.out.println(i + ".. 짝");
			}
			else {
				System.out.println(i);
			}
			
		}
		/*
